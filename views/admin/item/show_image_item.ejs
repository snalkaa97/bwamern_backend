<div
	class="tab-pane fade show active"
	id="image"
	role="tabpanel"
	aria-labelledby="show-image"
>
	<div class="row">
		<% items.imageId.map((item, index)=>{ console.log(item.imageUrl) %>
		<div class="col-md-4">
			<div class="card shadow mb-4 mt-2">
				<div class="card-body">
					<img
						src="http://localhost:3000/<%= item.imageUrl %>"
						alt="image"
						style="width: 100%; display: block"
					/>
				</div>
			</div>
		</div>
		<%})%>
	</div>
	<a href="/admin/item" class="btn btn-warning">Back</a>
</div>
